<UserControl x:Class="XLY.SF.WpfTest.HuZuoLin.UserControl1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:XLY.SF.WpfTest.HuZuoLin"
             xmlns:control="clr-namespace:XLY.SF.Project.Themes.CustromControl;assembly=XLY.SF.Project.Themes"
             mc:Ignorable="d" Background="Black">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Pack://application:,,,/XLY.SF.Project.Themes;Component/BaseThemes.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            
            
            
            
            
            
            
        </ResourceDictionary>
        
    </UserControl.Resources>
    <Grid>
        <StackPanel>
            <control:WebBrowserUc x:Name="web" Width="800" Height="800" Source="{Binding WbSource}"/>
            <Button VerticalAlignment="Bottom" Width="150" Height="30" Click="button_Click"></Button>
        </StackPanel>
    </Grid>
</UserControl>
